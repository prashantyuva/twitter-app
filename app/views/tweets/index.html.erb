<h1>Tweet#New</h1>
<div>
  <%= form_for @new_tweet do |f| %>
    <%= f.text_field :tweet %>
    <%= f.submit %>
  <% end %>
</div>

<h1>Tweets#List</h1>
<div>
  <% @tweets.each do |tweet| %>
    <sup><small><b><%= tweet.user.uid %></b></small></sup>
    <div><%= tweet.tweet %></div>
    <sub><small><%= follow_link(tweet.user) %></small></sub>
    <hr/>
  <% end %>
</div>
